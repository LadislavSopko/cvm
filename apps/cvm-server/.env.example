# MongoDB Configuration (required)
MONGODB_URI=mongodb://root:example@localhost:27017/cvm?authSource=admin

# Logging Configuration
CVM_LOG_LEVEL=info  # Options: debug, info, warn, error

# Execution Limits
CVM_MAX_EXECUTION_TIME=300000    # Maximum execution time in milliseconds (default: 5 minutes)
CVM_MAX_STACK_SIZE=1000          # Maximum stack depth (default: 1000)
CVM_MAX_OUTPUT_SIZE=1048576      # Maximum output buffer size in bytes (default: 1MB)

# Environment
NODE_ENV=development  # Options: development, production